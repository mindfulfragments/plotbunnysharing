<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter Your Idea Details</h1>
  <%= simple_form_for @idea do |f| %> 
    <%= f.input :title %>
    <%= f.input :summary %>
    <%= f.input :plot %>
    <%= f.input :characters %>
    <%= f.input :notes %>
    <br />
    <%= f.submit 'Create', class: 'btn btn-success' %>
  <% end %>
</div>